@{
    ViewData["Title"] = "Log1";
     Layout = "~/Views/Shared/ScreenLayout.cshtml";
}
<h1>Log</h1>

<link href="~/lib/datatables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
<link href="~/css/ScreenBack.css" rel="stylesheet" />
<div class="container">
    <br />
    <div style="width:90%; margin:0 auto;">
        <table id="logDatatable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>LogDate</th>
                    <th>Name</th>
                    <th>Surname</th>
                    <th>Page</th>
                    <th>Process</th>
                    <th>Action</th>
                </tr>
            </thead>
        </table>
                <br /><br /><br /><br /><br /><br />   
         @if (ViewBag.Usergruop == "Manager")
        {
            <button class="w3-button w3-blue  w3-large" > <a href ="/Manager/Manager/" style=" text-decoration: none;"><i class="fa fa-arrow-left"></i>  My Dashboard </a> </button>
        }
        else
        {
            <button class="w3-button w3-blue  w3-large" > <a href ="/Admin/Admin/" style=" text-decoration: none;"><i class="fa fa-arrow-left"></i>  My Dashboard </a> </button>
        }
            
    </div>
</div>

@section Scripts
{
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/log.js"></script>

}